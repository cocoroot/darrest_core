<p id="notice"><%= notice %></p>

<h1>Listing Manage Report Images</h1>

<table>
  <thead>
    <tr>
      <th>Report</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manage_report_images.each do |manage_report_image| %>
      <tr>
        <td><%= manage_report_image.report %></td>
        <td><%= manage_report_image.image %></td>
        <td><%= link_to 'Show', manage_report_image %></td>
        <td><%= link_to 'Edit', edit_manage_report_image_path(manage_report_image) %></td>
        <td><%= link_to 'Destroy', manage_report_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Report image', new_manage_report_image_path %>
