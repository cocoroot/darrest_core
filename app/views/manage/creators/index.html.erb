<p id="notice"><%= notice %></p>

<h1>Listing Manage Creators</h1>

<table>
  <thead>
    <tr>
      <th>Sid</th>
      <th>User</th>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manage_creators.each do |manage_creator| %>
      <tr>
        <td><%= manage_creator.sid %></td>
        <td><%= manage_creator.user %></td>
        <td><%= manage_creator.name %></td>
        <td><%= manage_creator.description %></td>
        <td><%= link_to 'Show', manage_creator %></td>
        <td><%= link_to 'Edit', edit_manage_creator_path(manage_creator) %></td>
        <td><%= link_to 'Destroy', manage_creator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Creator', new_manage_creator_path %>
