<p id="notice"><%= notice %></p>

<h1>Listing Core Collection Creations</h1>

<table>
  <thead>
    <tr>
      <th>Collection</th>
      <th>Creation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @core_collection_creations.each do |core_collection_creation| %>
      <tr>
        <td><%= core_collection_creation.collection %></td>
        <td><%= core_collection_creation.creation %></td>
        <td><%= link_to 'Show', core_collection_creation %></td>
        <td><%= link_to 'Edit', edit_core_collection_creation_path(core_collection_creation) %></td>
        <td><%= link_to 'Destroy', core_collection_creation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

