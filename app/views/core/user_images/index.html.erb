<p id="notice"><%= notice %></p>

<h1>Listing Core User Images</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Image</th>
      <th>In user</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @core_user_images.each do |core_user_image| %>
      <tr>
        <td><%= core_user_image.user %></td>
        <td><%= core_user_image.image %></td>
        <td><%= core_user_image.in_use %></td>
        <td><%= link_to 'Show', core_user_image %></td>
        <td><%= link_to 'Edit', edit_core_user_image_path(core_user_image) %></td>
        <td><%= link_to 'Destroy', core_user_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User image', new_core_user_image_path %>
