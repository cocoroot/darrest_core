<p id="notice"><%= notice %></p>

<h1>Listing Creator Tags</h1>

<table>
  <thead>
    <tr>
      <th>Creator</th>
      <th>Tag</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @creator_tags.each do |creator_tag| %>
      <tr>
        <td><%= creator_tag.creator %></td>
        <td><%= creator_tag.tag %></td>
        <td><%= link_to 'Show', creator_tag %></td>
        <td><%= link_to 'Edit', edit_creator_tag_path(creator_tag) %></td>
        <td><%= link_to 'Destroy', creator_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Creator tag', new_creator_tag_path %>
