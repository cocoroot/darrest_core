create_table :core_creations, id: :bigserial do |t|
  t.string :sid, limit: 20, null: false, default: '0'
  t.integer :creator_id, limit: 8, null: false, default: 0
  t.string :title, limit: 200
  t.string :description, limit: 10000
  t.string :license_code

  t.timestamps null: false
end

add_foreign_key :core_creations, :core_creators, column: 'creator_id', name: 'fk_creations_creator'
