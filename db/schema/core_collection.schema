create_table :core_collections, id: :bigserial do |t|
  t.string      :sid, limit:  20, null: false, default: '0'
  t.integer :user_id, limit:   8, null: false, default: 0
  t.string    :title, limit: 200

  t.timestamps null: false
end

add_foreign_key :core_collections, :core_users, column: 'user_id', name: 'fk_collections_users'
