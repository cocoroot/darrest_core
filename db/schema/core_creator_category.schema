create_table :core_creator_categories, id: :bigserial do |t|
  t.integer :creator_id, limit: 8, null: false, default: 0
  t.integer :category_id, limit: 8, null: false, default: 0

  t.timestamps null: false
end

add_foreign_key :core_creator_categories, :core_creators, column: 'creator_id', name: 'fk_creator_categories_creators'
add_foreign_key :core_creator_categories, :core_categories, column: 'category_id', name: 'fk_creator_categories_categories'
