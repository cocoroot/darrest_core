create_table :core_report_images, id: :bigserial do |t|
  t.integer :report_id, limit: 8, null: false, default: 0
  # Apache LimitRequestLine(default): 8190
  # max URL length available to IE: 2083
  t.string :image, limit: 2083

  t.timestamps null: false
end

add_foreign_key :core_report_images, :core_reports, column: 'report_id', name: 'fk_report_images_reports'
