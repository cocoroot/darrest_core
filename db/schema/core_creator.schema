create_table :core_creators, id: :bigserial do |t|
  t.string :sid, limit: 20, null: false, default: '0'
  t.integer :user_id, limit: 8, null: false, default: 0
  t.string :name, limit: 200, null: false, default: ''
  t.string :description, limit: 10000, null: true, default: nil

  t.timestamps null: false
end

add_foreign_key :core_creators, :core_users, column: 'user_id', name: 'fk_creators_users'
