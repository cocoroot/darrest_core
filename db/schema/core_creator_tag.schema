create_table :core_creator_tags, id: :bigserial do |t|
  t.integer :creator_id, limit: 8, null: false, default: 0
  t.integer :tag_id, limit: 8, null: false, default: 0

  t.timestamps null: false
end

add_foreign_key :core_creator_tags, :core_creators, column: 'creator_id', name: 'fk_creator_tags_creators'
add_foreign_key :core_creator_tags, :core_tags, column: 'tag_id', name: 'fk_creator_tags_tags'
